/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@shop/*": ["src/app/@public/*"],
      "@shop-core/*": ["src/app/@public/core/*"],
      "@admin/*": ["src/app/@admin/*"],
      "@admin-core/*": ["src/app/@admin/core/*"],
      "@core/*": ["src/app/@core/*"],
      "@shared/*": ["src/app/@shared/*"],
      "@graphql/*": ["src/app/@graphql/*"],
      "@gdata/*": ["src/assets/@data/*"],
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
